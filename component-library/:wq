[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	s = status
	co = checkout
	cob = checkout -b
	cmm = git add . && git commit -m
	l = log
	rs = restore .
